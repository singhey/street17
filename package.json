{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "devstart": "nodemon ./src/bin/www node --exec babel-node --presets env",
    "start": "npm run build && node ./build/bin/www",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s",
    "clean": "rm -rf build && mkdir build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "express-validator": "^6.2.0",
    "formidable": "^1.2.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.1",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
    
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2"
  }
}
